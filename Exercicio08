#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    int fat, num,aux;

    printf("Insira um valor para o qual deseja calcular seu fatorial: ");
    scanf("%d", &num);

    aux = num;

    for(fat = 1; num > 1; num = num - 1)
    fat = fat * num;

    printf("O fatorial de %d e %d\n", aux,fat);

    system("pause");
    return 0;
}
