#include <stdio.h>
#include<stdlib.h>

int main(void)
{
    int i,j,num,auxi=0,auxj=0,x;

    printf("Entre com um numero\n");
    scanf("%d", &num);
    printf("Entre com o i\n");
    scanf("%d",&i);
    printf("Entre com o j\n");
    scanf("%d",&j);

    printf("Os %d primeiros multiplos de %d ou de %d sao", num, i, j);

    for (x = 0; x < num; x++)
    {
    
    if (auxi < auxj)
	{
        printf(" %d", auxi);
        auxi = auxi + i;
	}
    
    else
	{
    printf(" %d",auxj);
    if (auxi == auxj)
    {
        auxi = auxi + i;
    }
        auxj = auxj + j;
	}
    
    }

    system("pause");
    return 0;
}
